package com.hoyotech.prison.bean;

import java.util.Date;

public class SpecialScore {
	
	private String score_id;          //专办评分ID
	private String score_pid;         //部门ID
	private String score_content;     //评分内容
	private int score_year;           //年份
	private int score_season;          //季度
	private String score_file;        //附件名
	private Date score_cmtTime;       //提交时间
	
	public String getScore_id() {
		return score_id;
	}
	public void setScore_id(String scoreId) {
		score_id = scoreId;
	}
	public String getScore_pid() {
		return score_pid;
	}
	public void setScore_pid(String scorePid) {
		score_pid = scorePid;
	}
	public String getScore_content() {
		return score_content;
	}
	public void setScore_content(String scoreContent) {
		score_content = scoreContent;
	}
	public int getScore_year() {
		return score_year;
	}
	public void setScore_year(int scoreYear) {
		score_year = scoreYear;
	}
	public int getScore_season() {
		return score_season;
	}
	public void setScore_season(int scoreSeason) {
		score_season = scoreSeason;
	}
	public String getScore_file() {
		return score_file;
	}
	public void setScore_file(String scoreFile) {
		score_file = scoreFile;
	}
	public Date getScore_cmtTime() {
		return score_cmtTime;
	}
	public void setScore_cmtTime(Date scoreCmtTime) {
		score_cmtTime = scoreCmtTime;
	}
	
	

}
