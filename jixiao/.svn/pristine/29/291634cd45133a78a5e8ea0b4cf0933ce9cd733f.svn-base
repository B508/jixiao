package com.hoyotech.prison.bean;

import java.util.Date;

public class JX_Appraise {
	private String id;//评价id
	private String content;//评价内容
	private String messageReleasesId;//评价信息id
	private Date appraise_time;//评价时间
	private String name;//评价人姓名
	private String phoneNumber;//电话号码
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getMessageReleasesId() {
		return messageReleasesId;
	}
	public void setMessageReleasesId(String messageReleasesId) {
		this.messageReleasesId = messageReleasesId;
	}
	public Date getAppraise_time() {
		return appraise_time;
	}
	public void setAppraise_time(Date appraiseTime) {
		appraise_time = appraiseTime;
	}

	public String getPhoneNumber() {
		return phoneNumber;
	}
	public void setPhoneNumber(String phoneNumber) {
		this.phoneNumber = phoneNumber;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getName() {
		return name;
	}
	
	
}
