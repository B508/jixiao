<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hoyotech.prison.bean">
	<class name="SocietyOpen" table="society_open">
		<comment></comment>
		<id name="id" column="id">
			<generator class="uuid.hex" />
		</id>
		<property name="startTime" type="java.util.Date">
			<column name="start_time" not-null="false" />
		</property>
		<property name="endTime" type="java.util.Date">
			<column name="end_time" not-null="false" />
		</property>
		<property name="type" type="string">
			<column name="type">
			</column>
		</property>
		<many-to-one name="approver" class="Police" fetch="select" lazy="false">
            <column name="approver" not-null="false" />
        </many-to-one>
		<property name="post" type="string">
			<column name="post">
			</column>
		</property>
		<property name="number" type="int">
			<column name="&quot;NUMBER&quot;">
			</column>
		</property>
		<property name="unit" type="string">
			<column name="unit">
			</column>
		</property>
		<property name="situation" type="string">
			<column name="situation">
			</column>
		</property>
		<property name="suggestion" type="string">
			<column name="suggestion">
			</column>
		</property>
		<property name="mark" type="string">
			<column name="mark">
			</column>
		</property>
		<property name="state" type="int">
			<column name="state" length="1" not-null="true" default="1">
			</column>
		</property>
		<property name="updateTime" type="java.util.Date">
			<column name="update_time" not-null="false" />
		</property>
		<property name="addTime" type="java.util.Date">
			<column name="add_time" not-null="true" />
		</property>
		<property name="prisonCode" type="java.lang.String">
			<column name="prison_code" not-null="false" />
		</property>
		<property name="isSync" type="int">
			<column name="is_sync" length="1" />
		</property>
	</class>
</hibernate-mapping>
