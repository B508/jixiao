			<result name="toPrint">/page/jixiaoEditor/jprint.jsp</result>
			<!--
				跳转到打印界面
			-->
			<result name="toPurpose">/page/jixiaoEditor/purpose.jsp</result>
			<!--
				跳转到工作目标界面
			-->
