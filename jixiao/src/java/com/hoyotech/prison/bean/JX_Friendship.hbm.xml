<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.hoyotech.prison.bean">
    <class name="JX_Friendship" table="JX_FRIENDLIST">
		<composite-id >  
            <key-property name="User1_id" column="USER1_ID"  type="java.lang.String"></key-property>  
            <key-property name="User2_id" column="USER2_ID"  type="java.lang.String"></key-property>  
     	</composite-id>
        
        <property name="isValid" type="java.lang.String">
            <column name="ISVALID" not-null="false"/>
        </property>        
    </class>
</hibernate-mapping>
