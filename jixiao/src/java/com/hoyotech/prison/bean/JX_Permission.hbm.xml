<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hoyotech.prison.bean">
	<class name="JX_Permission" table="JX_Permission">
		<comment></comment>
		<id name="id" column="id">
   			<generator class="uuid.hex" />
		</id>
		<property name="keyName" type="string">
			<column name="key_name" length="50">
				<comment></comment>
			</column>
		</property>
		<property name="permission" type="string">
			<column name="permission" length="50">
				<comment></comment>
			</column>
		</property>
		<property name="moduleId" type="string">
			<column name="module_id" length="50">
				<comment></comment>
			</column>
		</property>
		<property name="moduleName" type="string">
			<column name="module_name" length="50">
				<comment></comment>
			</column>
		</property>
		<property name="url" type="string">
			<column name="url" length="500">
				<comment></comment>
			</column>
		</property>
		
	</class>
</hibernate-mapping>
